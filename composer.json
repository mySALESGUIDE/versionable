{
    "name": "mySALESGUIDE/versionable-couchbase",
    "license": "MIT",
    "description": "Allows to create Laravel 5.4 Model versioning and restoring",
    "keywords": ["model", "laravel", "ardent", "version", "history", "restore", "couchbase"],
    "homepage": "http://github.com/mpociot/versionable",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mpociot/versionable/issues",
        "source": "https://github.com/mpociot/versionable"
    },
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "7.*",
        "ort-interactive-gmbh/laravel-couchbase": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "1.3.*",
        "illuminate/database": "7.*",
        "illuminate/events": "7.*",
        "orchestra/testbench": "5.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "": "src/"
        },
        "files": [
            "src/polyfill.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/VersionableTestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    }
}
